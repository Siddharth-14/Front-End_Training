<!DOCTYPE html>
<html lang="en">
    <%- include('particles/header'); -%>
  <body>
    <%- include('particles/navbar',{activeLink: "/about"}); -%>
    <div class="container">
      <div class="jumbotron">
        <h1><%= appName%></h1>
        <hr />
        <p>
          Welcome to <%= appName%>. You will find details of technical workshops
          happening nearby.
        </p>
        <h3>Features</h3>
        <ul class="list-group">
          <% features.forEach((feature) => {%>
          <li class="list-group-item list-group-item-primary"><%= feature %></li>
          <% }) %>
        </ul>
      </div>
    </div>
  </body>
</html>
